# Azure OpenAI Models
AZURE_OPENAI_LOCATION=westus

# Unified Azure OpenAI configuration
AZURE_OPENAI_RESOURCE=<your-openai-resource-name>
AZURE_OPENAI_KEY=<your-openai-key>
AZURE_OPENAI_API_VERSION=2024-12-01-preview

# Model deployment names
AZURE_OPENAI_MODEL_45=gpt-4.5-preview
AZURE_OPENAI_MODEL_41=gpt-4.1
AZURE_OPENAI_MODEL_4O=gpt-4o
AZURE_OPENAI_MODEL_O3_MINI=o3-mini
AZURE_OPENAI_MODEL_O3=o3
AZURE_OPENAI_MODEL_O1=o1
AZURE_OPENAI_MODEL_O4_MINI=o4-mini
AZURE_OPENAI_MODEL_O1_MINI=o1-mini
AZURE_OPENAI_MODEL_EMBEDDING_ADA=text-embedding-ada-002
AZURE_OPENAI_MODEL_EMBEDDING_SMALL=text-embedding-3-small
AZURE_OPENAI_MODEL_EMBEDDING_LARGE=text-embedding-3-large

# General OpenAI endpoint
AZURE_OPENAI_ENDPOINT=https://<your-openai-resource-name>.openai.azure.com/

# OpenAI Model
OPENAI_API_KEY=<your-openai-api-key>
OPENAI_MODEL_4O=gpt-4o
OPENAI_MODEL_O3=o3
OPENAI_MODEL_O3_MINI=o3-mini
OPENAI_MODEL_O1=o1
OPENAI_MODEL_O1_MINI=o1-mini
OPENAI_MODEL_O4_MINI=o4-mini
OPENAI_MODEL_EMBEDDING=text-embedding-3-large

# Azure AI Search
AZURE_AI_SEARCH_SERVICE_NAME=https://<your-search-name>.search.windows.net
AZURE_AI_SEARCH_API_KEY=<your-search-api-key>

# Azure Storage Account
AZURE_STORAGE_ACCOUNT_NAME=<your-storage-account-name>
AZURE_STORAGE_UPLOAD_JSON_CONTAINER_NAME=json-data
AZURE_STORAGE_UPLOAD_DOCUMENT_CONTAINER_NAME=document-data
AZURE_STORAGE_OUTPUT_CONTAINER_NAME=processed
AZURE_STORAGE_ACCOUNT_ID=/subscriptions/<subscription-id>/resourceGroups/<resource-group>/providers/Microsoft.Storage/storageAccounts/<storage-account-name>
AZURE_STORAGE_QUEUE_NAME=doc-process-queue
AZURE_STORAGE_BLOB_ENDPOINT=https://<your-storage-account-name>.blob.core.windows.net/
AZURE_STORAGE_QUEUE_ENDPOINT=https://<your-storage-account-name>.queue.core.windows.net/

# Azure AI Foundry
FOUNDRY_PROJECT="<region>.api.azureml.ms;<subscription-id>;<resource-group>;<foundry-project>"

# Application Insights Resource
APP_INSIGHTS_CONN_STRING="InstrumentationKey=<instrumentation-key>;IngestionEndpoint=https://<region>.in.applicationinsights.azure.com/;LiveEndpoint=https://<region>.livediagnostics.monitor.azure.com/;ApplicationId=<application-id>"

# Azure Container Apps
AZURE_SUBSCRIPTION_ID=<subscription-id>
AZURE_RESOURCE_GROUP=<resource-group>
AZURE_RG_LOCATION=<region>
AZURE_ACA_ENVIRONMENT=<environment-name>
AZURE_ACR_NAME=<acr-name>
AZURE_ACR_USERNAME=<acr-username>
AZURE_ACR_PASSWORD=<acr-password>

# Cosmos DB Settings
COSMOS_URI=https://<cosmos-account-name>.documents.azure.com:443/
COSMOS_KEY=<cosmos-key>
COSMOS_DB_NAME=proc-docs
COSMOS_CONTAINER_NAME=documents
COSMOS_CATEGORYID=categoryId
COSMOS_CATEGORYID_VALUE=documents
COSMOS_LOG_CONTAINER=logs

# Service Bus Settings
SERVICE_BUS_NAME=<service-bus-namespace>
SERVICE_BUS_QUEUE_NAME=document-processing-queue
SERVICE_BUS_CONNECTION_STRING=Endpoint=sb://<service-bus-namespace>.servicebus.windows.net/;SharedAccessKeyName=RootManageSharedAccessKey;SharedAccessKey=<access-key>

# User-Assigned Managed Identity
AZURE_CLIENT_ID=<client-id>
